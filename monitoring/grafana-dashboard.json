{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "API Overview",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "collapsed": false
    },
    {
      "title": "Request Rate",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 1 },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total[5m])) by (method)",
          "legendFormat": "{{ method }}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": { "lineWidth": 2, "fillOpacity": 15 }
        }
      }
    },
    {
      "title": "Error Rate",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 1 },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{status=~\"4..\"}[5m]))",
          "legendFormat": "4xx"
        },
        {
          "expr": "sum(rate(http_requests_total{status=~\"5..\"}[5m]))",
          "legendFormat": "5xx"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": { "lineWidth": 2, "fillOpacity": 15 },
          "thresholds": {
            "steps": [
              { "value": 0, "color": "green" },
              { "value": 1, "color": "yellow" },
              { "value": 5, "color": "red" }
            ]
          }
        }
      }
    },
    {
      "title": "Response Time (p95)",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 1 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p50"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": { "lineWidth": 2, "fillOpacity": 10 }
        }
      }
    },
    {
      "title": "Rate Limit Rejections",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 9 },
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{status=\"429\"}[5m]))",
          "legendFormat": "429/s"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "thresholds": {
            "steps": [
              { "value": 0, "color": "green" },
              { "value": 1, "color": "yellow" },
              { "value": 5, "color": "red" }
            ]
          }
        }
      }
    },
    {
      "title": "Active Connections",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 9 },
      "targets": [
        {
          "expr": "websocket_connections_active",
          "legendFormat": "WebSocket"
        }
      ]
    },
    {
      "title": "Uptime",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 9 },
      "targets": [
        {
          "expr": "time() - process_start_time_seconds",
          "legendFormat": "Uptime"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s" } }
    },
    {
      "title": "Memory Usage",
      "type": "stat",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 9 },
      "targets": [
        {
          "expr": "process_resident_memory_bytes",
          "legendFormat": "RSS"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "bytes" } }
    },
    {
      "title": "Workflow Executions",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 13 },
      "collapsed": false
    },
    {
      "title": "Execution Rate",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 14 },
      "targets": [
        {
          "expr": "sum(rate(workflow_executions_total[5m])) by (status)",
          "legendFormat": "{{ status }}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": { "lineWidth": 2, "fillOpacity": 15 }
        }
      }
    },
    {
      "title": "Execution Duration",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 14 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(workflow_execution_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.50, sum(rate(workflow_execution_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p50"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s" } }
    },
    {
      "title": "Running Executions",
      "type": "gauge",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 22 },
      "targets": [
        { "expr": "workflow_executions_running", "legendFormat": "Running" }
      ],
      "fieldConfig": {
        "defaults": {
          "max": 50,
          "thresholds": {
            "steps": [
              { "value": 0, "color": "green" },
              { "value": 20, "color": "yellow" },
              { "value": 40, "color": "red" }
            ]
          }
        }
      }
    },
    {
      "title": "Agents",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 28 },
      "collapsed": false
    },
    {
      "title": "Agent Status",
      "type": "piechart",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 29 },
      "targets": [
        { "expr": "agents_online", "legendFormat": "Online" },
        { "expr": "agents_total - agents_online", "legendFormat": "Offline" }
      ]
    },
    {
      "title": "Agent Heartbeats",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 16, "x": 8, "y": 29 },
      "targets": [
        {
          "expr": "sum(rate(agent_heartbeats_total[5m]))",
          "legendFormat": "Heartbeats/s"
        }
      ]
    },
    {
      "title": "Celery Workers",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 37 },
      "collapsed": false
    },
    {
      "title": "Queue Lengths",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 38 },
      "targets": [
        {
          "expr": "celery_queue_length",
          "legendFormat": "{{ queue }}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": { "lineWidth": 2, "fillOpacity": 20 }
        }
      }
    },
    {
      "title": "Task Processing Rate",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 38 },
      "targets": [
        {
          "expr": "sum(rate(celery_tasks_total[5m])) by (state)",
          "legendFormat": "{{ state }}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "ops" } }
    },
    {
      "title": "Database",
      "type": "row",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 46 },
      "collapsed": false
    },
    {
      "title": "DB Connection Pool",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 47 },
      "targets": [
        { "expr": "db_pool_size", "legendFormat": "Pool Size" },
        { "expr": "db_pool_checked_out", "legendFormat": "Checked Out" },
        { "expr": "db_pool_overflow", "legendFormat": "Overflow" }
      ]
    },
    {
      "title": "DB Query Duration",
      "type": "timeseries",
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 47 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(db_query_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s" } }
    }
  ],
  "refresh": "10s",
  "schemaVersion": 39,
  "tags": ["rpa", "monitoring"],
  "templating": { "list": [] },
  "time": { "from": "now-1h", "to": "now" },
  "timezone": "browser",
  "title": "RPA Engine Dashboard",
  "uid": "rpa-engine-main",
  "version": 1
}
